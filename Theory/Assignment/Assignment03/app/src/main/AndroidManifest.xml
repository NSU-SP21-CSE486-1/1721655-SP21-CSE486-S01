<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.studentsignupappv2">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.StudentSignupAppV2">
        <activity android:name=".uicontroller.Register"></activity>
        <activity android:name=".uicontroller.Login"></activity>
        <activity android:name=".uicontroller.StudentSearch">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.studentsignupappv2.uicontroller.StudentForm" />
        </activity>
        <activity android:name=".uicontroller.StudentList">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.studentsignupappv2.uicontroller.StudentForm" />
        </activity>
        <activity android:name=".uicontroller.StudentForm" />
        <activity android:name=".uicontroller.Address">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.studentsignupappv2.uicontroller.StudentForm" />
        </activity>
        <activity android:name=".uicontroller.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>